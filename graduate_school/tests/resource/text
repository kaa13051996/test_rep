<script type='text/javascript'>
  (function() {
  var changelogs_bell, dropdownModule, hide_changelogs_content, jsonpMarkAsRead, remove_circle, show_changelogs_content, tusur_cdn_next_siblings, tusur_cdn_parents, tusur_cdn_prev_siblings, tusur_cdn_siblings;

  tusur_cdn_prev_siblings = function(target) {
    var n, siblings;
    siblings = [];
    n = target;
    while (n = n.previousElementSibling) {
      siblings.push(n);
    }
    return siblings;
  };

  tusur_cdn_next_siblings = function(target) {
    var n, siblings;
    siblings = [];
    n = target;
    while (n = n.nextElementSibling) {
      siblings.push(n);
    }
    return siblings;
  };

  tusur_cdn_siblings = function(target) {
    var next, prev;
    prev = tusur_cdn_prev_siblings(target) || [];
    next = tusur_cdn_next_siblings(target) || [];
    return prev.concat(next);
  };

  tusur_cdn_parents = function(target, klass) {
    var elements;
    if (klass == null) {
      klass = null;
    }
    elements = [];
    while (target !== null) {
      target = target.parentNode;
      elements.unshift(target);
      if (klass !== null && target !== null && target.classList !== void 0) {
        if (target.classList.contains(klass)) {
          return elements;
        }
      }
    }
    return elements;
  };

  dropdownModule = function() {
    var all_dropdown_menu, all_dropdown_toggler, all_dropdown_toggler_hidden, document_onclick, dropdown_togglers, i, li, links, user_toggler;
    user_toggler = document.querySelector('.tusur-cdn-auth-block > ul > li.tusur-cdn-dropdown');
    all_dropdown_toggler = document.querySelector('.tusur-cdn-sites-list-toggler');
    all_dropdown_toggler_hidden = all_dropdown_toggler.offsetParent === null;
    all_dropdown_menu = document.querySelector('.tusur-cdn-sites-list ul');
    document_onclick = function(e) {
      var dropdown_li, i, opened_count, parents;
      parents = tusur_cdn_parents(e.target, 'opened');
      opened_count = document.querySelectorAll('.opened').length;
      if (parents[0] !== null && parents[0].classList.contains('opened') && opened_count === 1) {
        return;
      }
      i = 0;
      dropdown_li = document.querySelectorAll('.tusur-cdn-sites-list > ul > li');
      while (i < dropdown_li.length) {
        dropdown_li[i].classList.remove('opened');
        i++;
      }
      if (user_toggler !== null && parents[0] !== user_toggler) {
        user_toggler.classList.remove('opened');
      }
      if (all_dropdown_menu !== null && parents[0] !== all_dropdown_menu.parentElement) {
        all_dropdown_menu.parentElement.classList.remove('opened');
      }
    };
    document.ontouchstart = function(e) {
      document_onclick(e);
    };
    document.onclick = function(e) {
      document_onclick(e);
    };
    all_dropdown_toggler.onclick = function() {
      all_dropdown_menu.parentElement.classList.toggle('opened');
    };
    li = document.querySelectorAll('.tusur-cdn-sites-list > ul > li');
    i = 0;
    while (i < li.length) {
      li[i].classList.add('mobile');
      i++;
    }
    dropdown_togglers = document.querySelectorAll('.tusur-cdn-sites-list > ul > li > .block');
    i = 0;
    while (i < dropdown_togglers.length) {
      dropdown_togglers[i].onclick = function() {
        var dropdown_li, dropdown_menu, dropdown_siblings, j;
        dropdown_li = this.parentElement;
        dropdown_siblings = tusur_cdn_siblings(dropdown_li);
        j = 0;
        while (j < dropdown_siblings.length) {
          dropdown_siblings[j].classList.remove('opened');
          j++;
        }
        if (user_toggler !== null) {
          user_toggler.classList.remove('opened');
        }
        dropdown_menu = dropdown_li.querySelector('ul');
        if (dropdown_menu === void 0 || dropdown_menu === null) {
          return;
        }
        if (dropdown_li.classList.contains('opened')) {
          dropdown_li.classList.remove('opened');
          return;
        } else {
          dropdown_li.classList.add('opened');
        }
        if (all_dropdown_toggler_hidden) {
          return false;
        }
      };
      i++;
    }
    links = document.querySelectorAll('.tusur-cdn-sites-list > ul > li > ul > li > a');
    i = 0;
    while (i < links.length) {
      links[i].onclick = function() {
        return this.parentElement.parentElement.parentElement.classList.remove('opened');
      };
      i++;
    }
    if (user_toggler !== null) {
      user_toggler.classList.add('mobile');
      user_toggler.onclick = function() {
        this.classList.toggle('opened');
        li = document.querySelectorAll('.tusur-cdn-sites-list > ul > li');
        i = 0;
        while (i < li.length) {
          li[i].classList.remove('opened');
          i++;
        }
      };
    }
    if (!all_dropdown_toggler_hidden) {
      li = all_dropdown_menu.querySelectorAll('a');
      i = 0;
      while (i < li.length) {
        li[i].onclick = function() {
          all_dropdown_menu.parentElement.classList.remove('opened');
        };
        i++;
      }
    }
  };

  jsonpMarkAsRead = function() {
    var script, url;
    if (document.getElementById('close_change_log') === null) {
      return;
    }
    window.jsonpHandler = function() {
      if (arguments[0].ok === 'ok') {
        return remove_circle();
      }
    };
    script = document.createElement('script');
    url = document.getElementById('close_change_log').getAttribute('data-mark-as-read-url');
    script.src = url + "?callback=jsonpHandler";
    document.getElementsByTagName('head')[0].appendChild(script);
    return false;
  };

  show_changelogs_content = function(object, parent_object) {
    object[0].className += ' open';
    return parent_object.className += ' active';
  };

  hide_changelogs_content = function(object, parent_object) {
    object[0].classList.remove('open');
    return parent_object.classList.remove('active');
  };

  remove_circle = function() {
    return document.getElementsByClassName('tusur-cdn-changelogs-icons')[0].classList.remove('with-new-changes');
  };

  changelogs_bell = function() {
    var bell, changelogs_content, changelogs_wrapper, close_cross;
    changelogs_wrapper = document.getElementsByClassName('tusur-cdn-changelogs-icons')[0];
    changelogs_content = document.getElementsByClassName('tusur-cdn-changelogs-content');
    close_cross = document.getElementsByClassName('tusur-cdn-changelogs-content-close-cross');
    bell = document.getElementsByClassName('js-tusur-cdn-bell');
    if (bell[0]) {
      bell[0].addEventListener('click', function() {
        if (changelogs_content[0].classList.contains('open')) {
          return hide_changelogs_content(changelogs_content, changelogs_wrapper);
        } else {
          return show_changelogs_content(changelogs_content, changelogs_wrapper);
        }
      });
      return close_cross[0].addEventListener('click', function(event) {
        event.stopPropagation();
        hide_changelogs_content(changelogs_content, changelogs_wrapper);
        return jsonpMarkAsRead();
      });
    }
  };

  document.addEventListener('DOMContentLoaded', function() {
    var supportsTouch;
    supportsTouch = 'ontouchstart' in window || navigator.msMaxTouchPoints;
    changelogs_bell();
    if (!supportsTouch) {
      return;
    }
    dropdownModule();
  });

}).call(this);

</script>

